{
  "name": "react-wizard-step",
  "version": "0.1.3",
  "description": "React wizard step",
  "main": "lib/index.js",
  "scripts": {
    "start": "webpack-dev-server --hot --port=3000 --open --content-base example",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run clean-lib && npm run build-lib",
    "clean-lib": "rimraf lib/*",
    "build-lib": "babel src --out-dir lib --source-maps inline --copy-files",
    "bump-patch": "npm --no-git-tag-version version patch",
    "bump-minor": "npm --no-git-tag-version version minor",
    "bump-major": "npm --no-git-tag-version version major",
    "publish": "npm publish"
  },
  "author": "Jaturong Duangchaiyusuk",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/srankmeng/react-wizard-step.git"
  },
  "keywords": [
    "react",
    "wizard",
    "step",
    "es6",
    "steps",
    "react-component"
  ],
  "bugs": {
    "url": "https://github.com/srankmeng/react-wizard-step/issues"
  },
  "homepage": "https://github.com/srankmeng/react-wizard-step",
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "babel-core": "^6.8.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.6.1",
    "css-loader": "^0.28.7",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-flow-vars": "^0.5.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "^3.0.1",
    "node-sass": "^4.5.3",
    "path": "^0.12.7",
    "postcss-loader": "^2.0.6",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.6.2",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.7.1"
  },
